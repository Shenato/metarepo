import React from 'react'
import test from 'blue-tape'
import { getElementName } from '../src'

test('getObjectKeys', async (t) => {
  const obj = {
    foo: 'foo',
    bar: 'bar',
  }

  t.deepEquals(
    Object.keys(obj),
    ['foo', 'bar'],
    'should return keys'
  )
})

test('getElementName', async (t) => {
  const MyComp = () => null

  const NamedComp = () => null
  NamedComp.displayName = 'Named'

  t.equals(
    getElementName(<span />),
    'span',
    'should get name of primitive'
  )

  t.equals(
    getElementName(<MyComp />),
    'MyComp',
    'should get name of a function component'
  )

  t.equals(
    getElementName(<NamedComp />),
    'Named',
    'should get name from displayName'
  )
})
