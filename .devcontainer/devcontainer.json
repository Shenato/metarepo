{
	"name": "Node.js",
	"build": {
		"dockerfile": "Dockerfile",
		"args": { "VERSION": "14" }
	},
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash"
	},
	"extensions": [
    "dbaeumer.vscode-eslint",
    "editorconfig.editorconfig",
	],
	"forwardPorts": [3000],
	"postCreateCommand": "yarn install",
	"remoteUser": "node"
}
