    "mocku": "^0.5.0",
    "spyfn": "^0.2.0",
    "tap-diff": "^0.1.1",
    "typescript": "3.4.1"
  },
  "scripts": {
    "postinstall": "sh scripts/typescript.sh && rm node_modules/@types/node/console.d.ts > /dev/null 2>&1 || true"
  },
  "start": {
    "require": [
      [
        "@babel/register",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "targets": {
                  "node": "current"
                }
              }
            ],
            "@babel/preset-typescript",
            "@babel/preset-react"
          ],
          "plugins": [
            "@babel/plugin-syntax-dynamic-import",
            "babel-plugin-dynamic-import-node"
          ],
          "extensions": [
            ".tsx",
            ".ts"
          ]
        }
      ]
    ],
    "reporter": "@start/reporter-verbose"
  },
  "eslintConfig": {
    "extends": "@bubble-dev/eslint-config"
  },
  "eslintIgnore": [
    "build/",
    "coverage/",
    "static/",
    "node_modules/"
  ],
  "engines": {
    "node": ">=8.6.0"
  }
}
